{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": 0,
        "no-unused-vars": "warn",
        "func-names": 0,
        "indent": 0,
        "react/jsx-indent": 0,
        "react/jsx-props-no-spreading": 0,
        "react/jsx-indent-props": 0,
        "no-plusplus": 0,
    }
}
